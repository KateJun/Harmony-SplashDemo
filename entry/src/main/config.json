    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "portrait",
        "name": "com.harmony.mysplashdemo.SplashScreenAbility",
        "icon": "$media:icon",
        "description": "$string:splashscreenability_description",
        "label": "SplashDemo",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "portrait",
        "name": "com.harmony.mysplashdemo.MainAbility",
        "type": "page",
        "launchType": "standard"
      }
    ]
  }
}